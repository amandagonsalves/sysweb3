{
  "name": "@amandagonsalves/sysweb3-keyring",
  "version": "1.0.423",
  "main": "../../lib/keyring/cjs/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pollum-io/sysweb3.git"
  },
  "author": "Pollum Labs <pollum@pollum.io>",
  "license": "MIT",
  "files": [
    "lib/**/*"
  ],
  "types": "../../lib/keyring/cjs/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
      "build": "yarn && rm -rf ../../lib/keyring/esm && yarn build:esm && yarn build:cjs",
      "build:esm": "tsc -p ../../configs/tsconfig.esm.json --outDir ../../lib/keyring/esm --declarationDir ../../lib/keyring/esm/types && mv ../../lib/keyring/esm/index.js ../../lib/keyring/esm/index.mjs",
      "build:cjs": "tsc -p ../../configs/tsconfig.cjs.json --outDir ../../lib/keyring/cjs --declarationDir ../../lib/keyring/cjs/types && cp ../../lib/keyring/cjs/index.js ../../lib/keyring/index.js",
      "prepack": "yarn build",
      "release": "yarn build && release-it"
  },
  "dependencies": {
    "@release-it/keep-a-changelog": "^3.1.0",
    "release-it": "^15.6.0",
    "@metamask/obs-store": "^7.0.0",
    "@metamask/safe-event-emitter": "^2.0.0",
    "@amandagonsalves/sysweb3-core": "^1.0.19",
    "@amandagonsalves/sysweb3-network": "^1.0.73",
    "@amandagonsalves/sysweb3-utils": "^1.1.214",
    "axios": "^0.26.1",
    "bip39": "^3.0.4",
    "coinselectsyscoin": "^1.0.77",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "eth-chains": "^1.0.0",
    "eth-sig-util": "^3.0.1",
    "ethereum-cryptography": "^1.1.2",
    "ethereumjs-util": "^7.1.5",
    "ethereumjs-wallet": "^1.0.2",
    "ethers": "^5.6.4",
    "ethers-eip712": "^0.2.0",
    "graphql": "^16.3.0",
    "graphql-request": "^4.1.0",
    "lodash": "^4.17.21",
    "syscoinjs-lib": "^1.0.214",
    "syscointx-js": "^1.0.101",
    "web3": "^1.7.1",
    "web3-utils": "^1.7.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.23"
  }
}